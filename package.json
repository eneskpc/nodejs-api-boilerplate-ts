{
  "name": "node-typescript-express-mongodb",
  "version": "1.0.0",
  "description": "Boilerplate for Node-TypeScript-Express-MongoDB",
  "repository": {
    "type": "git",
    "url": "https://github.com/kvvignesh/Node-TypeScript-Express-MongoDB"
  },
  "keywords": [
    "TypeScript",
    "express",
    "nodejs",
    "boilerplate",
    "eslint",
    "mongodb",
    "swagger",
    "openapi",
    "JWT"
  ],
  "author": "K V Vignesh",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "nodemon src/server.ts",
    "start": "node dist/src/server.js",
    "test": "jest",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.2",
    "express": "^4.17.1",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.0",
    "moment": "^2.27.0",
    "mongoose": "^8.1.0",
    "nodemon": "^3.0.3",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/hapi__joi": "^17.1.3",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.7.29",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-express": "^1.12.0",
    "mockingoose": "^2.13.2",
    "prettier": "3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "license": "MIT"
}
